{
  "name": "staircase-import-formatter",
  "displayName": "Staircase Imports",
  "description": "Format your library imports in a staircase pattern",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.0.0"
  },
  "publisher": "MyPing0",
  "author": {
    "name": "MyPing0",
    "email": "mypingo7@yahoo.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MyPingO/staircase-imports"
  },
  "keywords": [
    "staircase",
    "imports",
    "formatter",
    "python",
    "vscode",
    "extension",
    "MyPing0"
  ],
  "icon": "icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "staircase-import-formatter.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "title": "Staircase Import Formatter",
      "properties": {
        "Staircase Import Formatter.order": {
          "type": "string",
          "default": "ascending",
          "enum": [
            "ascending",
            "descending"
          ],
          "description": "Choose between ascending or descending order for import statements."
        }
      }
    },
    "scripts": {
      "lint": "eslint .",
      "pretest": "npm run lint",
      "test": "vscode-test"
    },
    "devDependencies": {
      "@types/vscode": "^1.85.0",
      "@types/mocha": "^10.0.6",
      "@types/node": "18.x",
      "eslint": "^8.56.0",
      "typescript": "^5.3.3",
      "@vscode/test-cli": "^0.0.4",
      "@vscode/test-electron": "^2.3.9"
    }
  }
}
